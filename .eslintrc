{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "mocha": true
  },
  "settings": {
    "ecmascript": 6
  },
  "plugins": [ ],
  "rules": {
    "strict": 1,
    "no-console": 0,
    "max-len": [2, 100, 2, {ignoreUrls: true} ],
    "no-unused-vars": 1,
    "camelcase": 0,
    "no-underscore-dangle": 0
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "ecmaFeatures": true
  },
  "extends": [
    "defaults/configurations/walmart/es5-node"
  ]
}
